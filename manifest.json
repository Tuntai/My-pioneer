{
	"manifest_version":2,
	"name":"One stop",
	"version":"1.0",
	"description": "A chrome extension",
	"icons":{
		"128":"icons/icon128.png",
		"48" :"icons/icon48.png",
		"16":"icons/icon16.png"
	},
	"browser_action":{
		"default_icon":"icons/icon16.png",
		"default_popup":"popup.html"
	},

	"permissions": ["storage",
    "tabs",
    "notifications",
    "desktopCapture"
  ],
  "web_accessible_resources": [
       
  ],
  "background": {
    "scripts": [
        "js/background.js"
    ]
},
  "options_ui": {
    "chrome_style": true,
    "page": "options.html",
    "open_in_tab": false
  }
}